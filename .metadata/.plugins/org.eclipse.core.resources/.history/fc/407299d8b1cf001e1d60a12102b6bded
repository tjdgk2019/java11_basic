package org.kh.java;

public class Variable7 {
	public static void main(String[] args) {
		String name = "이성하";
		Integer age = 29;
		Float height = 161.8f;
		boolean pass = true;
		double weight = 0;
		
		System.out.println("name : "+name.getClass().getName());
		System.out.println("age : "+name.getClass().getName());
		System.out.println("height : "+name.getClass().getName());
	}
}
